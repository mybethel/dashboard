{
  "name": "@bethel/dashboard",
  "version": "0.1.0",
  "scripts": {
    "postinstall": "npm run build",
    "start": "npm run dev",
    "dev": "webpack-dev-server --inline --hot --content-base ./dist",
    "build": "NODE_ENV=production webpack",
    "lint": "eslint --ext .js,.vue .",
    "predeploy": "now switch bethel -t $NOW_TOKEN",
